{
    "name": "front",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "node index.js",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build && npm run server:build:ts",
        "server:build:ts": "node node_modules/typescript/bin/tsc --project server/tsconfig.json",
        "server:start": "node server/App.js",
        "server:watch:ts": "node node_modules/typescript/bin/tsc --project server/tsconfig.json -w"
    },
    "dependencies": {
        "bootstrap": "^4.4.1",
        "bootstrap-vue": "^2.10.1",
        "core-js": "^3.6.4",
        "register-service-worker": "^1.6.2",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.2",
        "vue-property-decorator": "^8.3.0",
        "vue-router": "^3.1.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.4",
        "@types/node": "^13.11.0",
        "@vue/cli-plugin-babel": "^4.2.0",
        "@vue/cli-plugin-pwa": "^4.2.0",
        "@vue/cli-plugin-router": "^4.2.0",
        "@vue/cli-plugin-typescript": "^4.2.0",
        "@vue/cli-service": "^4.2.0",
        "express": "^4.18.2",
        "typescript": "~3.7.5",
        "vue-template-compiler": "^2.6.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
